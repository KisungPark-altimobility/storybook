        "eslint-config-next": "13.3.0",
        "next": "13.2.4",
        "next-themes": "^0.2.1",
        "react": "18.2.0",
        "react-aria": "^3.23.0",
        "react-dom": "18.2.0",
        "react-stately": "^3.18.0",
        "react-use-measure": "2.1.1",
        "typescript": "5.0.2"
    },
    "devDependencies": {
