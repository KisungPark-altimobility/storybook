        "@swc/cli": "0.1.55",
        "@swc/core": "1.2.124",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.0",
        "eslint-plugin-storybook": "^0.6.11",
        "jest": "^29.5.0",
        "jest-css-modules-transform": "^4.4.2",
        "jest-environment-jsdom": "^29.5.0",
        "storybook": "^7.0.4",
