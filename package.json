{
    "name": "@kisungpark-altimobility/storybook",
    "description": "Storybook design systems",
    "version": "0.1.2",
    "license": "ISC",
    "repository": "https://github.com/kisungpark-altimobility/storybook",
    "private": true,
    "source": "src/index.ts",
    "main": "src/index.ts",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "deploy-storybook": "storybook-to-ghpages",
        "emit-types": "tsc -p tsconfig.build.json",
        "clean": "rm -rf dist/",
        "clean:dist": "rm -rf dist/app dist/stories",
        "build:swc": "npx swc src --copy-files -d dist && npm run emit-types && npm run clean:dist",
        "prepublishOnly": "npm run clean && npm run build:swc"
    },
    "dependencies": {
        "@radix-ui/react-popover": "^1.0.5",
